{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-devit-training"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"GitHubTest_credential": {
			"type": "secureString",
			"metadata": "Secure string for 'credential' of 'GitHubTest'"
		},
		"Mahesh_Test_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Mahesh_Test'"
		},
		"GitHubTest_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "mreddyag"
		},
		"Mahesh_Test_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "CENTAURI\\SQL2019"
		},
		"Mahesh_Test_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Mahesh_test"
		},
		"Mahesh_Test_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-devit-training_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"GitHubTest_credential": {
						"value": "[parameters('GitHubTest_credential')]"
					},
					"Mahesh_Test_password": {
						"value": "[parameters('Mahesh_Test_password')]"
					},
					"GitHubTest_properties_typeProperties_username": {
						"value": "[parameters('GitHubTest_properties_typeProperties_username')]"
					},
					"Mahesh_Test_properties_typeProperties_server": {
						"value": "[parameters('Mahesh_Test_properties_typeProperties_server')]"
					},
					"Mahesh_Test_properties_typeProperties_database": {
						"value": "[parameters('Mahesh_Test_properties_typeProperties_database')]"
					},
					"Mahesh_Test_properties_typeProperties_userName": {
						"value": "[parameters('Mahesh_Test_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}